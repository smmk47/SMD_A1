<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c2f3f0"
    tools:context=".MainActivity23">



    <View
        android:id="@+id/firstCircle"

        android:background="@drawable/circle"

        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="-70dp"
        android:layout_marginBottom="0dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"



        />

    <View
        android:id="@+id/SecondCircle"

        android:background="@drawable/circle"

        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="-70dp"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/backButtfdon"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:layout_margin="16dp"
        android:layout_marginStart="130dp"
        android:background="@drawable/backarrow"
        android:backgroundTint="#156F77"
        android:padding="8dp"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="227dp"
        android:layout_height="42dp"
        android:text="Booked Sessions"
        android:textColor="#054942"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.117" />


    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="300dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <!-- First CardView -->
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView25"
            android:layout_width="151dp"
            android:layout_height="115dp"
            android:layout_margin="8dp"
            app:cardBackgroundColor="#d9d9d9"
            app:cardCornerRadius="16dp"
            app:cardElevation="4dp"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <!-- TextViews for Card Content -->
            </RelativeLayout>

        </androidx.cardview.widget.CardView>

        <!-- Bottom View for First Card -->
        <View
            android:id="@+id/viewCardBottom256"
            android:layout_width="179dp"
            android:layout_height="69dp"
            android:layout_margin="8dp"
            android:background="@drawable/cardbottom" />

        <!-- Second CardView -->

        <!-- Bottom View for Second Card -->

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.833"
        tools:layout_editor_absoluteX="-16dp">

        <!-- First CardView -->
        <androidx.cardview.widget.CardView
            android:id="@+id/cardView2b5"
            android:layout_width="wrap_content"
            android:layout_height="109dp"
            android:layout_margin="8dp"
            app:cardBackgroundColor="#d9d9d9"
            app:cardCornerRadius="16dp"
            app:cardElevation="4dp"
            app:cardUseCompatPadding="true">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <!-- Add your ImageView -->
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:src="@drawable/rectangle15" />


            </RelativeLayout>

        </androidx.cardview.widget.CardView>

        <!-- Bottom View for First Card -->
        <View
            android:id="@+id/viewCardBottom25"
            android:layout_width="179dp"
            android:layout_height="69dp"
            android:layout_margin="8dp"
            android:background="@drawable/cardbottom" />

        <!-- Second CardView -->

        <!-- Bottom View for Second Card -->

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>