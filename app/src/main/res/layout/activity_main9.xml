<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="#ddf7f6"
tools:context=".MainActivity9">




<View
    android:id="@+id/firstCircle"

    android:layout_width="150dp"

    android:layout_height="150dp"
    android:layout_marginTop="-70dp"
    android:layout_marginBottom="0dp"
    android:background="@drawable/circle"

    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"


    />

<View
    android:id="@+id/SecondCircle"

    android:layout_width="150dp"

    android:layout_height="150dp"
    android:layout_marginLeft="-70dp"
    android:layout_marginTop="0dp"
    android:background="@drawable/circle"

    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<Button
    android:id="@+id/backButtfdon"
    android:layout_width="45dp"
    android:layout_height="44dp"
    android:layout_margin="16dp"
    android:layout_marginStart="130dp"
    android:layout_marginTop="16dp"
    android:background="@drawable/backarrow"
    android:backgroundTint="#156F77"
    android:padding="8dp"
    android:textColorHighlight="#FFFFFF"
    android:textColorHint="#FFFFFF"
    android:textColorLink="#FFFFFF"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.048"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />


<View
    android:id="@+id/circleView"
    android:layout_width="100dp"
    android:layout_height="100dp"
    android:layout_marginTop="676dp"
    android:background="@drawable/navicircle"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<View
    android:id="@+id/rectangleView"
    android:layout_width="411dp"
    android:layout_height="79dp"
    android:layout_marginBottom="4dp"
    android:background="@drawable/navibar"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.0"
    app:layout_constraintStart_toStartOf="parent" />


<Button
    android:id="@+id/button17"
    android:layout_width="76dp"
    android:layout_height="64dp"
    android:layout_marginStart="10dp"
    android:layout_marginTop="10dp"
    android:background="@android:color/transparent"
    android:drawableTop="@drawable/homeicon"
    android:gravity="bottom|center_horizontal"
    android:hapticFeedbackEnabled="false"
    android:text="Home"

    android:textAlignment="center"
    android:textSize="10sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.015"
    app:layout_constraintStart_toStartOf="@+id/rectangleView"
    app:layout_constraintTop_toTopOf="@+id/rectangleView" />

<Button
    android:id="@+id/button18"
    android:layout_width="82dp"
    android:layout_height="64dp"
    android:layout_marginStart="10dp"
    android:layout_marginTop="10dp"
    android:background="@android:color/transparent"
    android:drawableTop="@drawable/searchicon"
    android:gravity="bottom|center_horizontal"

    android:text="search"
    android:textSize="10sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.292"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="@+id/rectangleView" />

<Button
    android:id="@+id/button20"
    android:layout_width="80dp"
    android:layout_height="67dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="10dp"
    android:background="@android:color/transparent"
    android:drawableTop="@drawable/chaticon"
    android:gravity="bottom|center_horizontal"

    android:text="chat"
    android:textSize="10sp"
    app:layout_constraintEnd_toStartOf="@+id/button21"
    app:layout_constraintTop_toTopOf="@+id/rectangleView" />

<Button
    android:id="@+id/button21"
    android:layout_width="79dp"
    android:layout_height="66dp"
    android:layout_marginTop="10dp"
    android:layout_marginEnd="10dp"
    android:background="@android:color/transparent"
    android:drawableTop="@drawable/profileicon"
    android:gravity="bottom|center_horizontal"
    android:text="profile"
    android:textSize="10sp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.984"
    app:layout_constraintStart_toStartOf="@+id/rectangleView"
    app:layout_constraintTop_toTopOf="@+id/rectangleView" />

<Button
    android:id="@+id/button22"
    android:layout_width="88dp"
    android:layout_height="77dp"
    android:layout_marginBottom="40dp"
    android:background="@android:color/transparent"
    android:drawableTop="@drawable/uploadicon"
    android:gravity="bottom|center_horizontal"
    android:text="upload"
    android:textSize="10sp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent" />


<TextView
    android:id="@+id/textView26"
    android:layout_width="213dp"
    android:layout_height="53dp"
    android:layout_marginTop="80dp"
    android:text="Search Results"
    android:textColor="#156f77"
    android:textSize="30sp"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.151"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<LinearLayout
    android:id="@+id/linearLayout"
    android:layout_width="397dp"
    android:layout_height="137dp"
    android:layout_marginBottom="248dp"
    android:orientation="horizontal"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintStart_toStartOf="parent">

    <!-- Third CardView -->
    <androidx.cardview.widget.CardView
        android:id="@+id/cardView27"
        android:layout_width="151dp"
        android:layout_height="115dp"
        android:layout_margin="8dp"
        app:cardBackgroundColor="#d9d9d9"
        app:cardCornerRadius="16dp"
        app:cardElevation="4dp"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <!-- TextViews for Card Content -->
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <!-- Bottom View for Third Card -->
    <View
        android:id="@+id/viewCardBottom27"
        android:layout_width="179dp"
        android:layout_height="69dp"
        android:layout_margin="8dp"
        android:background="@drawable/cardbottom" />

    <!-- Fourth CardView -->

    <!-- Bottom View for Fourth Card -->

</LinearLayout>

<LinearLayout
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    android:orientation="horizontal"
    app:layout_constraintBottom_toTopOf="@+id/linearLayout"
    tools:layout_editor_absoluteX="0dp">

    <!-- First CardView -->
    <androidx.cardview.widget.CardView
        android:id="@+id/cardView25"
        android:layout_width="151dp"
        android:layout_height="115dp"
        android:layout_margin="8dp"
        app:cardBackgroundColor="#d9d9d9"
        app:cardCornerRadius="16dp"
        app:cardElevation="4dp"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <!-- TextViews for Card Content -->
        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <!-- Bottom View for First Card -->
    <View
        android:id="@+id/viewCardBottom256"
        android:layout_width="179dp"
        android:layout_height="69dp"
        android:layout_margin="8dp"
        android:background="@drawable/cardbottom" />

    <!-- Second CardView -->

    <!-- Bottom View for Second Card -->

</LinearLayout>

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="1.0"
    tools:layout_editor_absoluteX="0dp">

    <!-- First CardView -->
    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2b5"
        android:layout_width="151dp"
        android:layout_height="115dp"
        android:layout_margin="8dp"
        app:cardBackgroundColor="#d9d9d9"
        app:cardCornerRadius="16dp"
        app:cardElevation="4dp"
        app:cardUseCompatPadding="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <!-- Bottom View for First Card -->
    <View
        android:id="@+id/viewCardBottom25"
        android:layout_width="179dp"
        android:layout_height="69dp"
        android:layout_margin="8dp"
        android:background="@drawable/cardbottom" />



</LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
